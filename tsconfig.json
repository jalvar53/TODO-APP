{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "typeRoots": [
            "node_modules/@types"
          ],
        "lib": [
            "es2015", "dom"
        ],
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true
    }
}
